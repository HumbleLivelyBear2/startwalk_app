<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(return_method) -%>">
  <span class='title'>
    <%= return_method.method1 %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), return_method_path(return_method),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_return_method_path(return_method),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_return_method_path(return_method),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'shared.admin.delete', :title => return_method.method1),
        :method => :delete %>
  </span>
</li>
